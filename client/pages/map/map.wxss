/* client/pages/map/map.wxss */
page {
  height: 100%;
}

map {
  width: 100%;
  min-height: 100%;
  position: relative;

}

.homing {
  background-color: #fff;
  width: 40px;
  height: 40px;
  border-radius: 3px;
  box-shadow: 0px 0px 10px 0px #f0f0f0;
  position: absolute;
  right: 10px;
  top: 50%;
  transform: translate(0, -50%);
}

.img {
  width: 30px;
  height: 30px;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
}